<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.garkin.accountms">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.UserMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".dao.AccountContentProvider"
            android:authorities="com.example.garkin.accountms"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".activity.IncomeBrowseActivity" />
        <activity android:name=".activity.IncomeEditActivity" />
        <activity android:name=".activity.NoteBrowseActivity" />
        <activity android:name=".activity.NoteEditActivity" />
        <activity android:name=".activity.PaymentBrowseActivity" />
        <activity android:name=".activity.PaymentEditActivity" />
        <activity android:name=".activity.UseFinanceActivity" />
        <activity android:name=".activity.UserLoginActivity" />
        <activity android:name=".activity.UserPasswordActivity"></activity>
    </application>

</manifest>